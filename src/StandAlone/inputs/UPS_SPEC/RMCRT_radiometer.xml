      <locationsMin            spec="REQUIRED VECTOR  'positive'"/>
      <locationsMax            spec="REQUIRED VECTOR  'positive'"/>
      <nRays                   spec="REQUIRED INTEGER 'positive'"/> 
    </Radiometer>
</Uintah_Include>
