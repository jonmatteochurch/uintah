<?xml version='1.0' encoding='ISO-8859-1' ?>
<Uintah_Include>      
   <Radiometer                 spec="OPTIONAL NO_DATA"     attribute1="type OPTIONAL STRING 'float, double'">
      <viewAngle               spec="REQUIRED DOUBLE  'positive'"/>
